import com.hcsc.entity.KidsDetails;
import com.hcsc.pojo.PlanData;

public interface DataCollectionServiceInter {

	public String genRandomCaseIdSearchByUsrId(Integer appId);

	public Map<Integer, String> getPlans();

	public String savePlanData(@RequestBody PlanData plans);

	public String saveCitizenCase(Case caseData);

	public String saveIncomeDetails(IncomeDetails details);

	public String saveKidsDetails(KidsDetails kitDetails);

	public String saveEducationDetails(EducationDetails educationDetails);

	public Map<String, String> getIncomeDetails(Integer caseId);
}
