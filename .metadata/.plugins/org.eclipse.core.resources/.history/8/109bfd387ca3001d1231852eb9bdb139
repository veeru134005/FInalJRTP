package com.hcsc.controller;

import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.PathVariable;
import org.springframework.web.bind.annotation.RestController;

import com.hcsc.service.DataCollectionServiceInter;

@RestController
public class CitizenController {
	
	
	private DataCollectionServiceInter dataService;
	
	
	@GetMapping(name = "/citizen/{appId}")
	public String genRandomCaseIdBySearchByUsrId(@PathVariable Integer appId) {
		
		return dataService.genRandomCaseIdBySearchByUsrId(appId);
		
	}

}
